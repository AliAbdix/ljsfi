Executable = "gbb";
InputSandbox = {"SCRIPTPATH/sw-mgr","SCRIPTPATH/gbb","SCRIPTPATH/lcg-tags"};
OutputSandbox = {"stdout", "stderr", "install.xml"};
stdoutput = "stdout";
stderror = "stderr";
Arguments = "--timing -v -m 300 -p -D 600 -s GBB_SURL sw-mgr --test RELNAME -l LOGPATH/RELMAIN -p PHYSPATH/VERSIONAREA -P SWNAME -T p1patch -t ARCH -O opt --require-prj REQPRJ --host CENAME --subcluster SCNAME --vo atlas --tags RELTAG -m PACMANVERSION --xml install.xml KV_POST_OPT DBREL";
Environment = {"LCG_CATALOG_TYPE=LCGCATALOGTYPE","LFC_HOST=LFCHOST"};
VirtualOrganisation = "atlas";
Requirements = other.GlueCEUniqueId=="CEID";
