Executable = "gbb";
InputSandbox = {"@SCRIPTPATH@/sw-mgr","@SCRIPTPATH@/gbb","@SCRIPTPATH@/lcg-tags"};
OutputSandbox = {"stdout", "stderr", "install.xml"};
stdoutput = "stdout";
stderror = "stderr";
Arguments = "--timing -v -m 300 -p -D 600 -s @GBB_SURL@ sw-mgr -i @RELNAME@ -l @OSGLOGICALPATH@/@RELNAME@ -p @OSGPHYSICALPATH@/@VERSIONAREA@ -o --validate --ddm-setup $OSG_APP/atlas_app/atlas_rel/ddm/latest/setup.sh --use-grid --grid-name @GRIDNAME@ --replicate --snapname @PACKAGE@ --release-ver @SWREVISION@ --host @TAGHOST@ --vo atlas --require-shfs --disk-free @DISKSPACE@ --add-tags --tags @RELTAG@ -P @SWNAME@ -T release -t @ARCH@ -O opt -m @PACMANVERSION@ --xml install.xml --site-config $OSG_APP/atlas_app/atlas_rel/AtlasSiteConfig.sh --dump-tags $OSG_APP/atlas_app/atlas_rel/tags --post-siteinfo @KVPOST@ @KITCACHE@ @DBRELEASE@ @ATLASCOMPILER@ @PACBALLDATASET@ @PACBALLNAME@";
Environment = {"LCG_CATALOG_TYPE=@LCGCATALOGTYPE@","LFC_HOST=@LFCHOST@"};
VirtualOrganisation = "atlas";
Requirements = other.GlueCEUniqueId=="@SITE_CS@";
